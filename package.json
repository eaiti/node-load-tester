{
  "name": "node-load-tester",
  "version": "1.0.0",
  "description": "A simple load testing tool for HTTP endpoints",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:example": "ts-node src/index.ts examples/config.example.json",
    "dev:basic": "ts-node src/index.ts examples/config.basic-auth.json",
    "dev:apikey": "ts-node src/index.ts examples/config.apikey.json",
    "dev:custom": "ts-node src/index.ts examples/config.custom-auth.json",
    "dev:post": "ts-node src/index.ts examples/config.post.json",
    "dev:put": "ts-node src/index.ts examples/config.put.json",
    "dev:patch": "ts-node src/index.ts examples/config.patch.json",
    "dev:delete": "ts-node src/index.ts examples/config.delete.json",
    "dev:head": "ts-node src/index.ts examples/config.head.json",
    "dev:options": "ts-node src/index.ts examples/config.options.json",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"*.json\" \"*.md\" \"examples/**/*.json\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"*.json\" \"*.md\" \"examples/**/*.json\"",
    "check": "npm run lint && npm run format:check && npm test"
  },
  "keywords": [
    "load-testing",
    "http",
    "performance"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^15.15.0",
    "jest": "^29.5.0",
    "nock": "^13.3.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "ansi-escapes": "^7.0.0",
    "axios": "^1.6.0",
    "cli-progress": "^3.12.0"
  }
}
